{
  "private": true,
  "sideEffects": false,
  "type": "module",
  "name": "my-remix-app",
  "scripts": {
    "build": "remix build",
    "dev": "sst bind 'remix dev -c \"npm run dev:server\" --manual'",
    "dev:server": "nodemon --exec \"ts-node --esm ./server.ts\" --watch ./server.ts",
    "start": "cross-env NODE_ENV=production ts-node --esm ./server.ts",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@remix-run/css-bundle": "1.19.0-pre.2",
    "@remix-run/express": "1.19.0-pre.2",
    "@remix-run/node": "1.19.0-pre.2",
    "@remix-run/react": "1.19.0-pre.2",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "isbot": "^3.6.8",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@remix-run/dev": "1.19.0-pre.2",
    "@remix-run/eslint-config": "1.19.0-pre.2",
    "@swc/core": "^1.3.63",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "chokidar": "^3.5.3",
    "eslint": "^8.38.0",
    "nodemon": "^2.0.22",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
